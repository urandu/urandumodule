<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>urandumodule</id>
	<name>Urandu Module Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.urandumodule</package>
	<author>dtx</author>
	<description>
		My bootcamp module
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/urandumodule/update.rdf</updateURL>
	<require_version>1.11.0</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.urandumodule.UranduModuleActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.urandumodule.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		uranduModule.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with urandumodule. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

